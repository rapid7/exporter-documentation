{* Get youtube block payload *}
{[ const block = context /]}

{* Create youtube embed block *}
{[ if block.url ]}
<div class="embed-responsive embed-responsive-16by9" class="embed-url">
  <iframe class="embed-responsive-item" src="{{ block.url }}" allowfullscreen></iframe>
</div>
{[ if block.caption ]}
    <p class="caption">{{ block.caption }}</p>
{[/]}
{[/]}
